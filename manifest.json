{
  "manifest_version": 3,
  "name": "ðŸ“Š Data Scraper Tool",
  "version": "1.2.5",
  "description": "CÃ´ng cá»¥ scrape vÃ  export dá»¯ liá»‡u tá»« trang web",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon.png",
      "48": "icons/icon.png",
      "128": "icons/icon.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": [
        "config.js",
        "services/dom-utils.js",
        "services/selector-utils.js",
        "services/extraction-utils.js",
        "services/state-manager.js",
        "services/progress-indicator.js",
        "services/product-formatter.js",
        "utils.js",
        "core/api-scraper.js",
        "core/base-scraper.js",
        "core/pagination-handler.js",
        "core/product-scraper.js",
        "core/scrape-workflow.js",
        "core/detail-scraper.js",
        "core/detail-extractors.js",
        "core/list-scraper.js",
        "handlers/highlight-manager.js",
        "handlers/message-handler.js",
        "content.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "storage",
    "downloads",
    "webRequest"
  ],
  "host_permissions": ["*://*/*"],
  "web_accessible_resources": [
    {
      "resources": ["test/data.test"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  }
}